 FontSize->16,ExpressionUUID->"ea419374-766b-4638-9c53-922ce4ef5029"],

Cell[BoxData[
 RowBox[{
  RowBox[{"lenAintC", "[", "r_", "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"A", ",", "Cr", ",", " ", "AinterC"}], "}"}], ",", " ", 
    "\[IndentingNewLine]", 
    "                                                 ", 
    RowBox[{
     RowBox[{"Cr", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{"k", ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "r"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     "                                                  ", 
     RowBox[{"A", "=", " ", 
      RowBox[{"Select", "[", 
       RowBox[{"Cr", ",", "EvenQ"}], "]"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "this", " ", "selects", " ", "even", " ", "numbers", " ", "in", " ", 
       "Cr"}], "*)"}], "\[IndentingNewLine]", 
     "                                                  ", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{"A", " ", "=", " ", 
        RowBox[{"Select", "[", 
         RowBox[{"Cr", ",", " ", 
          RowBox[{
           RowBox[{"Divisible", "[", 
            RowBox[{"#", ",", "2"}], "]"}], "&"}]}], "]"}]}], ";"}], "*)"}], 
     " ", "\[IndentingNewLine]", 
     "                                                  ", 
     RowBox[{"AinterC", " ", "=", " ", 
      RowBox[{"Intersection", "[", 
       RowBox[{"A", ",", " ", "Cr"}], "]"}]}], ";", " ", 
     RowBox[{"Print", "[", "A", "]"}], ";", "\[IndentingNewLine]", 
     "                                                  ", 
     RowBox[{"Length", "[", "AinterC", "]"}]}]}], "\[IndentingNewLine]", 
   "                             ", "]"}]}]], "Input",
