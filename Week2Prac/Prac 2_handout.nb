Cell[BoxData[
 RowBox[{
  RowBox[{"transposeAB", "[", 
   RowBox[{"size_", ",", "range_", ",", "nrun_"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"A", ",", "B", ",", 
      RowBox[{"n", "=", "size"}], ",", " ", 
      RowBox[{"r", "=", "range"}], ",", " ", 
      RowBox[{"count", "=", "0"}], ",", " ", 
      RowBox[{"f", "=", "0"}], ",", " ", 
      RowBox[{"run", " ", "=", " ", "nrun"}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", "\[IndentingNewLine]", "                    ", 
      RowBox[{
       RowBox[{
        RowBox[{"A", " ", "=", " ", 
         RowBox[{"randM", "[", 
          RowBox[{"n", ",", "r"}], "]"}]}], ";", "\[IndentingNewLine]", 
        "                    ", 
        RowBox[{"B", " ", "=", " ", 
         RowBox[{"randM", "[", 
          RowBox[{"n", ",", "r"}], "]"}]}], ";", "\[IndentingNewLine]", 
        "                    ", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{"A", ".", "B"}], ")"}], "\[Transpose]"}], "\[Equal]", 
           " ", 
           RowBox[{
            RowBox[{"A", "\[Transpose]"}], ".", 
            RowBox[{"B", "\[Transpose]"}]}]}], ",", " ", 
          RowBox[{"count", "++"}], ",", " ", 
          RowBox[{"f", "++"}]}], "]"}]}], ",", "\[IndentingNewLine]", 
       "          ", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{
          RowBox[{"Print", "[", 
           RowBox[{"MatrixForm", "[", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"A", ".", "B"}], ")"}], "\[Transpose]"}], "]"}], "]"}], 
          ";", " ", 
          RowBox[{"Print", "[", 
           RowBox[{"MatrixForm", "[", 
            RowBox[{
             RowBox[{"A", "\[Transpose]"}], ".", 
             RowBox[{"B", "\[Transpose]"}]}], "]"}], "]"}]}], ","}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"{", "run", "}"}]}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"{", 
       RowBox[{"count", ",", "f"}], "}"}], "]"}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"N", "[", 
      RowBox[{"count", "/", "run"}], "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
