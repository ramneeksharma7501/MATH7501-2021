Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"biSection", "[", 
     RowBox[{"func_", ",", 
      RowBox[{"{", 
       RowBox[{"a_", ",", "b_"}], "}"}], ",", 
      RowBox[{"eps_", "?", "Positive"}]}], "]"}], "/;", 
    RowBox[{
     RowBox[{
      RowBox[{"func", "[", "a", "]"}], 
      RowBox[{"func", "[", "b", "]"}]}], "<", "0"}]}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"c", ",", "d", ",", "e"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{"c", ",", "d"}], "}"}], "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"func", "[", "a", "]"}], ">", "0"}], ",", 
         RowBox[{"{", 
          RowBox[{"b", ",", "a"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"a", ",", "b"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", "   ", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Abs", "[", 
          RowBox[{"d", "-", "c"}], "]"}], ">", "eps"}], ",", "  ", 
        RowBox[{"(*", " ", 
         RowBox[{
         "eps", " ", "is", " ", "the", " ", "tolerance", " ", "level"}], 
         "*)"}], "\[IndentingNewLine]", "     ", 
        RowBox[{"With", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"e", "=", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"c", "+", "d"}], ")"}], "/", "2"}]}], "}"}], ",", "   ", 
          RowBox[{"(*", 
           RowBox[{"this", " ", "divides", " ", "the", " ", 
            RowBox[{"interval", " ", "[", 
             RowBox[{"c", ",", "d"}], "]"}], " ", "into", " ", "half"}], " ", 
           "*)"}], "\[IndentingNewLine]", "       ", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"func", "[", "e", "]"}], "<", "0"}], ",", 
            RowBox[{"c", "=", "e"}], ",", 
            RowBox[{"d", "=", "e"}]}], "]"}]}], "\[IndentingNewLine]", "   ", 
         "]"}]}], "\[IndentingNewLine]", " ", "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"c", ",", "d"}], "}"}]}]}], "   ", 
    RowBox[{"(*", " ", 
     RowBox[{
     "this", " ", "is", " ", "the", " ", "interval", " ", "containing", " ", 
      "the", " ", "approximate", " ", "solution"}], "*)"}], 
    "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]"}]], "Input",
 CellChangeTimes->{{3.827690719923477*^9, 3.8276907199249983`*^9}, 
